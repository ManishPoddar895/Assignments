
{
    "AWSTemplateFormatVersion": "2010-09-09",
    "Description": "CloudFormation template for creating LambdaFunction of reporting tool",
		
    "Resources":{
	
        "ReportingTool": {
			"Type": "AWS::Lambda::Function",
			"Properties": {
				"Handler": "handler",
				"Role": "arn:aws:iam::346319152574:role/ReportingToolRole",
				"Code": {
					"S3Bucket": "devops-core-reporting-tools",
					"S3Key": "ZipFile"
				},
				"Runtime": "python3.6",
				"Timeout": "timeout"
			}
		}
    }
}
